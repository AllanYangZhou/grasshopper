{
  "manifest_version": 2,

  "name": "CalHacks",
  "description": "This extension will send the current URL to another extension",
  "version": "1.0",

  "browser_action": {
   "default_icon": "icon.png",
   "default_popup": "popup.html"
  },
  "permissions": [
   "activeTab", "gcm", "tabs",
   "storage", "downloads"
   ]
}